@model RedyFitness.Dto.AccountCreation
@{
    ViewBag.Title = "CreateAccount";
}

<h2>Create Account</h2>

<h2>Username</h2>
<div class=" form-group-sm">
    @Html.TextBoxFor(x => x.userName, new { @class = "form-control" })
</div><br />

<h2>Password</h2>
<div class="form-group-sm">
    @Html.TextBoxFor(x => x.userPassword, new { @class = "form-control" })<br />
</div>

